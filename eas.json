{
  "cli": {
    "version": ">= 0.38.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_TOKEN": "${EXPO_TOKEN}"
      }
    },
    "production": {
      "env": {
        "EXPO_TOKEN": "${EXPO_TOKEN}"
      }
    }
  }
}
